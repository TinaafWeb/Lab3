@{
    Layout = "_Layout";
}

<h3>Display Dynamic Data</h3>
<table>
    <tr>
        <td>
            First Name:
        </td>
        <td>
            @Model.fname

        </td>
    </tr>
    <tr>
        <td>
            Last Name:
        </td>
        <td>
            @Model.lname
        </td>
    </tr>
    <tr>
        <td>
            Age:
        </td>
        <td>
            @Model.age
        </td>
    </tr>
    <tr>
        <td>
            Email:
        </td>
        <td>
            @Model.email
        </td>
    </tr>
    <tr>
        <td>
            Date:
        </td>
        <td>
            @Model.date
        </td>
    </tr>
    <tr>
        <td>
            Password:
        </td>
        <td>
            @Model.pwd
        </td>
    </tr>
    <tr>
        <td>
            Description:
        </td>
        <td>
            @Model.dis
        </td>
    </tr>

</table>

